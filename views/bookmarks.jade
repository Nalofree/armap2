extends layout
block content
  #content
    // Закладки
    #bookmarks.container
      // С классом bm-none нет закладок
      article#my-bookmarks
        section#breadcrumbs
          a(href='#') Поиск
          span >
          a(href='#') Результаты поиска
          span >
          a.active(href='#') Закладки
        section#bm-header
          h2 Мои закладки
        section#bm-items
          //- - for (var i = 0; i < offices.length; i++) {
          //-   offices[i]
          //- }
          - if (count == 0)
            section.bm-none
              div
                h2 Закладок нет
                //- a(href='search.html') Перейти к списку
          - else
            -for (var i = 0; i < offices.length; i++)
              section.bm-item
                .bm-item-img
                  a(href='office-'+offices[i].office_id)
                    img(src='images/obj/'+offices[i].image_filename)
                .bm-item-desc
                  h3
                    - var name = decodeURI(offices[i].office_name) ? decodeURI(offices[i].office_name) : 'Error!'
                      a(href='office-'+offices[i].office_id)= name
                  section
                    .bm-item-price
                      p
                        | Цена за м
                        sup 2
                        | : 
                        span= offices[i].office_subprice +' р.'
                    .bm-item-square
                      p
                        | Площадь: 
                        span= offices[i].office_area +' м'
                          sup 2
                a.bm-delete(href='#', data-title=offices[i].office_id)
      // Рекомендуемые
